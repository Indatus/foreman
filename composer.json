{
    "name": "indatus/foreman",
    "description": "Template based Laravel app builder",
    "keywords": ["laravel builder", "laravel template", "foreman"],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Webb",
            "email": "brianwebb01@gmail.com"
        }
    ],
    "autoload": {
        "classmap": ["src/lib"],
        "files": [
            "src/vendor/illuminate/support/Illuminate/Support/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*@dev",
        "kherge/amend": "~3.0",
        "symfony/console": "2.4.*",
        "symfony/process": "2.4.*",
        "illuminate/filesystem": "4.2.*@dev"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "hot/phpunit-runner": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendor"
    },
    "minimum-stability": "stable"
}